{
  "name": "api-lang-chain",
  "type": "module",
  "version": "0.0.2",
  "description": "produce a natural language interface for your api",
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.umd.js"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/teaguestockwell/api-lang-chain"
  },
  "bugs": {
    "url": "https://github.com/teaguestockwell/api-lang-chain/issues"
  },
  "homepage": "https://github.com/teaguestockwell/api-lang-chain",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "microbundle --sourcemap false --tsconfig tsconfig.json --visualize",
    "test": "tsc && node --require tslib --test build/**/*.test.js",
    "lint": "prettier -c src && eslint src",
    "lint:fix": "prettier --write src/",
    "prepare": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "author": {
    "name": "Teague Stockwell",
    "email": "tsappdevelopment@gmail.com",
    "url": "https://teaguestockwell.com"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "husky": "^8.0.1",
    "microbundle": "^0.15.0",
    "prettier": "^3.0.2",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.7.2"
  }
}
